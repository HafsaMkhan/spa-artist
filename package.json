    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run format",
      "npm run lint:fix",
      "git add --all"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
